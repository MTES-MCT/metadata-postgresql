@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geodcat: <http://data.europa.eu/930/> .
@prefix gsp: <http://www.opengis.net/ont/geosparql#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdo: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix snum: <http://snum.scenari-community.org/Metadata/Vocabulaire/#> .
@prefix uuid: <urn:uuid:> .

snum:DatasetShape
	a sh:NodeShape ;
    sh:targetClass dcat:Dataset ;
    sh:qualifiedMinCount 1 ;
    sh:property [ 
		sh:path dct:title ;
		sh:name "Libellé"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
		sh:minCount 1 ;
        sh:uniqueLang true ;
        sh:order 0 ;
        snum:widget "QLineEdit" ;
	] ;
    sh:property [ 
		sh:path dct:description ;
		sh:name "Description"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
		sh:minCount 1 ;
        sh:uniqueLang true ;
        sh:order 1 ;
        snum:widget "QTextEdit" ;
        snum:rowSpan 15 ;
	] ;
    sh:property [ 
		sh:path dct:modified ;
		sh:name "Dernière modification"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:dateTime ;
        sh:maxCount 1 ;
        sh:order 2 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}T[0-9]{2}[:][0-9]{2}[:][0-9]{2}([.][0-9]{6})?([+][0-9]{2}[:][0-9]{2}([:][0-9]{2}([.][0-9]{6})?)?)?$" ;
        snum:widget "QDateTimeEdit" ;
	] ;
    sh:property [ 
		sh:path snum:isExternal ;
		sh:name "Donnée externe"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:boolean ;
        sh:maxCount 1 ;
        sh:order 3 ;
        snum:widget "QCheckBox" ;
	] ;
    sh:property [ 
		sh:path dcat:theme ;
		sh:name "Thème"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        snum:ontology <http://publications.europa.eu/resource/authority/data-theme>,
            <https://inspire.ec.europa.eu/theme> ;
        sh:datatype xsd:string ;
        sh:order 4 ;
        snum:widget "QComboBox" ;
	] ;
    sh:property [ 
		sh:path dcat:keyword ;
		sh:name "Mots-clé libres"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:order 5 ;
        snum:widget "QLineEdit" ;
	] ;
    sh:property [ 
		sh:path dct:temporal ;
		sh:name "Couverture temporelle"@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dct:PeriodOfTime ;
        sh:order 6 ;
	] ;   
    sh:property [ 
		sh:path dct:provenance ;
		sh:name "Généalogie"@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dct:ProvenanceStatement ;
        sh:order 7 ;
	] ;
    sh:property [ 
		sh:path dct:accessRights ;
		sh:name "Conditions d'accès"@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:RightsStatement ;
        sh:maxCount 1 ;
        sh:order 8 ;
        snum:ontology <http://inspire.ec.europa.eu/metadata-codelist/LimitationsOnPublicAccess> ;
        snum:widget "QComboBox" ;
	] ;
    sh:property [ 
		sh:path dcat:contactPoint ;
		sh:name "Point de contact"@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class vcard:Kind ;
        sh:order 10 ;
	] ;
    sh:property [ 
		sh:path dct:publisher ;
		sh:name "Diffuseur"@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:maxCount 1 ;
        sh:order 11 ;
	] ;    
	sh:property [ 
		sh:path dcat:distribution ;
		sh:name "Distribution"@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dcat:Distribution ;
        sh:order 20 ;
	] ;
    sh:property [ 
		sh:path dcat:landingPage ;
		sh:name "Page internet"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class foaf:Document ;
        sh:order 30 ;
        snum:widget "QLineEdit" ;
	] ;
    sh:property [ 
		sh:path dct:language ;
		sh:name "Langue des données"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class dct:LinguisticSystem ;
        sh:defaultValue <http://publications.europa.eu/resource/authority/language/FRA> ;
        sh:order 40 ;
        snum:ontology <http://publications.europa.eu/resource/authority/language> ;
        snum:widget "QComboBox" ;
	] ;
    sh:property [ 
		sh:path snum:relevanceScore ;
		sh:name "Score"@fr ;
        sh:description "plus le score est élevé plus la donnée est mise en avant dans les résultats de recherche"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:integer ;
        sh:maxCount 1 ;
        sh:order 50 ;
        snum:widget "QLineEdit" ;
	] .
    
snum:DistributionShape
	a sh:NodeShape ;
    sh:targetClass dcat:Distribution ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ 
		sh:path dct:accessURL ;
		sh:name "URL d'accès"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class rdfs:Resource ;
        sh:order 1 ;
        snum:widget "QLineEdit" ;
	] ;
    sh:property [ 
		sh:path dct:issued ;
		sh:name "Date de publication"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 2 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        snum:widget "QDateEdit" ;
        snum:inputMask "0000-00-00" ;
	] ;
    sh:property [ 
		sh:path dct:rights ;
		sh:name "Propriété intellectuelle"@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dct:RightsStatement ;
        sh:maxCount 1 ;
        sh:order 3 ;
	] ;
    sh:property [ 
		sh:path dct:license ;
		sh:name "Licence"@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:LicenseDocument ;
        sh:maxCount 1 ;
        sh:order 4 ;
        snum:ontology <http://spdx.org/licenses/> ;
        snum:widget "QComboBox" ;
	] .
    
snum:RightsStatementShape
    a sh:NodeShape ;
    sh:targetClass dct:RightsStatement ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ 
		sh:path rdfs:label ;
		sh:name "Mention"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:uniqueLang true ;
        sh:order 1 ;
        snum:widget "QTextEdit" ;
        snum:rowSpan 4 ;
	] .
    
snum:LicenseDocumentShape
    a sh:NodeShape ;
    sh:targetClass dct:LicenseDocument ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ 
		sh:path dct:type ;
		sh:name "Type"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:order 1 ;
        snum:ontology <http://purl.org/adms/licencetype/1.1> ;
        snum:widget "QComboBox" ;
	] ;
    sh:property [ 
		sh:path rdfs:label ;
		sh:name "Termes"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:uniqueLang true ;
        sh:order 2 ;
        snum:widget "QTextEdit" ;
	] .
    
snum:ProvenanceStatementShape
    a sh:NodeShape ;
    sh:targetClass dct:ProvenanceStatement ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ 
		sh:path rdfs:label ;
		sh:name "Texte"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:uniqueLang true ;
        sh:order 0 ;
        snum:widget "QTextEdit" ;
        snum:rowSpan 20 ;
	] .
    
snum:PeriodOfTimeShape
    a sh:NodeShape ;
    sh:targetClass dct:PeriodOfTime ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ 
		sh:path dcat:startDate ;
		sh:name "Date de début"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 0 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        snum:widget "QDateEdit" ;
        snum:inputMask "0000-00-00" ;
	] ;
    sh:property [ 
		sh:path dcat:endDate ;
		sh:name "Date de fin"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 1 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        snum:widget "QDateEdit" ;
        snum:inputMask "0000-00-00" ;
	] .
    
snum:KindShape
    a sh:NodeShape ;
    sh:targetClass vcard:Kind ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ 
		sh:path vcard:fn ;
		sh:name "Nom"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:order 1 ;
        snum:widget "QLineEdit" ;
	] ;
    sh:property [ 
		sh:path vcard:hasEmail ;
		sh:name "Courriel"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class owl:Thing ;
        sh:maxCount 1 ;
        sh:order 2 ;
        snum:widget "QLineEdit" ;
        snum:transform "email" ;
	] ;
    sh:property [
		sh:path vcard:hasTelephone ;
		sh:name "Téléphone"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class owl:Thing ;
        sh:maxCount 1 ;
        sh:order 3 ;
        snum:widget "QLineEdit" ;
        snum:transform "phone" ;
        snum:placeholder "+33-1-23-45-67-89" ;
	] ;
    sh:property [ 
		sh:path vcard:hasURL ;
		sh:name "Site internet"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class owl:Thing ;
        sh:maxCount 1 ;
        sh:order 4 ;
        snum:widget "QLineEdit" ;
	] ;
    sh:property [ 
		sh:path vcard:organization-name ;
		sh:name "Organisme"@fr ;
        sh:description "le cas échéant, organisation plus vaste dont le point de contact fait partie"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:order 5 ;
        snum:widget "QLineEdit" ;
	] .

snum:AgentShape
    a sh:NodeShape ;
    sh:targetClass foaf:Agent ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ 
		sh:path foaf:name ;
		sh:name "Nom"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:order 0 ;
        snum:widget "QLineEdit" ;
	] ;
    sh:property [ 
		sh:path dct:type ;
		sh:name "Type"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:order 1 ;
        snum:ontology <http://purl.org/adms/publishertype/1.0> ;
        snum:widget "QComboBox" ;
	] ;
    sh:property [ 
		sh:path foaf:mbox ;
		sh:name "Courriel"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class owl:Thing ;
        sh:maxCount 1 ;
        sh:order 2 ;
        snum:widget "QLineEdit" ;
        snum:transform "email" ;
	] ;
    sh:property [
		sh:path foaf:phone ;
		sh:name "Téléphone"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class owl:Thing ;
        sh:maxCount 1 ;
        sh:order 3 ;
        snum:widget "QLineEdit" ;
        snum:transform "phone" ;
        snum:placeholder "+33-1-23-45-67-89" ;
	] ;
    sh:property [ 
		sh:path foaf:workplaceHomepage ;
		sh:name "Site internet"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class foaf:Document ;
        sh:maxCount 1 ;
        sh:order 4 ;
        snum:widget "QLineEdit" ;
	] .
    
