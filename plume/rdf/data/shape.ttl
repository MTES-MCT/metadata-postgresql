@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix cnt: <http://www.w3.org/2011/content#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix geodcat: <http://data.europa.eu/930/> .
@prefix gsp: <http://www.opengis.net/ont/geosparql#> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sdmx-attribute: <http://purl.org/linked-data/sdmx/2009/attribute#> .
@prefix sdo: <http://schema.org/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix plume: <http://registre.data.developpement-durable.gouv.fr/plume/> .
@prefix uuid: <urn:uuid:> .

plume:DatasetShape
    a sh:NodeShape ;
    sh:targetClass dcat:Dataset ;
    sh:qualifiedMinCount 1 ;
    sh:property [
        sh:path dct:title ;
        sh:name "Libellé"@fr ;
        sh:description "Nom explicite du jeu de données."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:minCount 1 ;
        sh:uniqueLang true ;
        sh:order 0 ;
    ] ;
    sh:property [
        sh:path owl:versionInfo ;
        sh:name "Version"@fr ;
        sh:description "Numéro de version ou millésime du jeu de données."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:order 1 ;
    ] ;
    sh:property [
        sh:path dct:description ;
        sh:name "Description"@fr ;
        sh:description "Description du jeu de données."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:minCount 1 ;
        sh:uniqueLang true ;
        sh:order 2 ;
        plume:longText true ;
        plume:rowSpan 15 ;
    ] ;
    sh:property [
        sh:path plume:isExternal ;
        sh:name "Donnée externe"@fr ;
        sh:description "Ce jeu de données est-il la reproduction de données produites par un tiers ? Une donnée issue d'une source externe mais ayant fait l'objet d'améliorations notables n'est plus une donnée externe."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:boolean ;
        sh:maxCount 1 ;
        sh:order 3 ;
    ] ;
    sh:property [
        sh:path dcat:theme ;
        sh:name "Thèmes"@fr ;
        sh:description "Classification thématique du jeu de données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        plume:ontology <http://publications.europa.eu/resource/authority/data-theme>,
            <http://inspire.ec.europa.eu/theme>,
            <http://inspire.ec.europa.eu/metadata-codelist/TopicCategory>,
            <http://registre.data.developpement-durable.gouv.fr/ecospheres/themes-ecospheres>,
            <http://inspire.ec.europa.eu/metadata-codelist/SpatialScope> ;
        sh:order 4 ;
    ] ;
    sh:property [
        sh:path dcat:keyword ;
        sh:name "Mots-clés libres"@fr ;
        sh:description "Mots ou très brèves expressions représentatives du jeu de données, à l'usage des moteurs de recherche."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:order 6 ;
    ] ;
    sh:property [
        sh:path dct:spatial ;
        sh:name "Couverture géographique"@fr ;
        sh:description "Territoire·s décrit·s par le jeu de données."@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:Location ;
        sh:order 9 ;
        plume:ontology <http://registre.data.developpement-durable.gouv.fr/plume/EuAdministrativeTerritoryUnitFrance>,
            <http://id.insee.fr/geo/departement>,
            <http://id.insee.fr/geo/region>,
            <http://registre.data.developpement-durable.gouv.fr/plume/InseeIndividualTerritory>,
            <http://publications.europa.eu/resource/authority/atu>,
            <http://id.insee.fr/geo/commune> ;
    ] ;
    sh:property [
        sh:path dct:temporal ;
        sh:name "Couverture temporelle"@fr ;
        sh:description "Période·s décrite·s par le jeu de données. La date de début et la date de fin peuvent être confondues, par exemple dans le cas de l'extraction ponctuelle d'une base mise à jour au fil de l'eau."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dct:PeriodOfTime ;
        sh:order 10 ;
    ] ;
    sh:property [
        sh:path dct:created ;
        sh:name "Date de création"@fr ;
        sh:description "Date de création du jeu de données. Il peut par exemple s'agir de la date de création de la table PostgreSQL ou de la date de la première saisie de données dans cette table."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 11 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        plume:inputMask "9999-99-99" ;
    ] ;
    sh:property [
        sh:path dct:modified ;
        sh:name "Date de dernière modification"@fr ;
        sh:description "Date de la dernière modification du jeu de données. Cette date est présumée tenir compte tant des modifications de fond, tels que les ajouts d'enregistrements, que des modification purement formelles (corrections de coquilles dans les données, changement de nom d'un champ, etc.). L'absence de date de dernière modification signifie que la donnée n'a jamais été modifiée depuis sa création."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 12 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        plume:inputMask "9999-99-99" ;
    ] ;
    sh:property [
        sh:path dct:issued ;
        sh:name "Date de publication"@fr ;
        sh:description "Date à laquelle le jeu de données a été diffusé. Cette date ne devrait être renseignée que pour un jeu de données effectivement mis à disposition du public ou d'utilisateur tiers via un catalogue de données ou un site internet. Pour un jeu de données mis à jour en continu, il s'agit de la date de publication initiale."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 13 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        plume:inputMask "9999-99-99" ;
    ] ;
    sh:property [
        sh:path dct:accrualPeriodicity ;
        sh:name "Fréquence de mise à jour"@fr ;
        sh:description "Fréquence de mise à jour des données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class dct:Frequency ;
        sh:maxCount 1 ;
        plume:ontology <http://inspire.ec.europa.eu/metadata-codelist/MaintenanceFrequency>,
            <http://publications.europa.eu/resource/authority/frequency> ;
        sh:order 14 ;
    ] ;
    sh:property [
        sh:path adms:status ;
        sh:name "Statut"@fr ;
        sh:description "Maturité du jeu de données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:order 15 ;
        plume:ontology <http://publications.europa.eu/resource/authority/dataset-status>,
            <http://registre.data.developpement-durable.gouv.fr/plume/ISO19139ProgressCode> ;
    ] ;
    sh:property [
        sh:path dct:provenance ;
        sh:name "Généalogie"@fr ;
        sh:description "Sources et méthodes mises en œuvre pour produire les données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dct:ProvenanceStatement ;
        sh:order 20 ;
    ] ;
    sh:property [
        sh:path adms:versionNotes ;
        sh:name "Note de version"@fr ;
        sh:description "Différences entre la version courante des données et les versions antérieures."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        sh:order 21 ;
        plume:longText true ;
    ] ;
    sh:property [
        sh:path dct:conformsTo ;
        sh:name "Conforme à"@fr ;
        sh:description "Standard, schéma, référentiel de coordonnées, etc. auquel se conforment les données."@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:Standard ;
        sh:order 22 ;
        plume:ontology <http://registre.data.developpement-durable.gouv.fr/plume/OgcEpsgFrance>,
            <http://www.opengis.net/def/crs/EPSG/0>,
            <http://registre.data.developpement-durable.gouv.fr/plume/IgnCrs> ;
        plume:compute "manual" ;
    ] ;
    sh:property [
        sh:path dcat:spatialResolutionInMeters ;
        sh:name "Résolution spatiale en mètres"@fr ;
        sh:description "Plus petite distance significative dans le contexte du jeu de données, exprimée en mètres."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:decimal ;
        sh:maxCount 1 ;
        sh:order 24 ;
    ] ;
    sh:property [
        sh:path dqv:hasQualityMeasurement ;
        sh:name "Résolution spatiale"@fr ;
        sh:description "Spécification du niveau de détail des données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dqv:QualityMeasurement ;
        sh:order 25 ;
    ] ;
    sh:property [
        sh:path dcat:temporalResolution ;
        sh:name "Résolution temporelle"@fr ;
        sh:description "Plus petit pas de temps significatif dans le contexte du jeu de données."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:duration ;
        sh:maxCount 1 ;
        sh:order 26 ;
    ] ;
    sh:property [
        sh:path dct:accessRights ;
        sh:name "Conditions d'accès"@fr ;
        sh:description "Contraintes réglementaires limitant l'accès aux données."@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:RightsStatement ;
        sh:order 30 ;
        plume:ontology <http://inspire.ec.europa.eu/metadata-codelist/LimitationsOnPublicAccess>,
            <http://publications.europa.eu/resource/authority/access-right>,
            <http://registre.data.developpement-durable.gouv.fr/plume/CrpaAccessLimitations> ;
    ] ;
    sh:property [
        sh:path dcat:contactPoint ;
        sh:name "Point de contact"@fr ;
        sh:description "Entité à contacter pour obtenir des informations sur les données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class vcard:Kind ;
        sh:order 40 ;
    ] ;
    sh:property [
        sh:path dct:publisher ;
        sh:name "Éditeur"@fr ;
        sh:description "Organisme ou personne qui assure la publication des données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:maxCount 1 ;
        sh:order 41 ;
    ] ;
    sh:property [
        sh:path dct:creator ;
        sh:name "Auteur"@fr ;
        sh:description "Principal responsable de la production des données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:maxCount 1 ;
        sh:order 42 ;
    ] ;
    sh:property [
        sh:path dct:rightsHolder ;
        sh:name "Propriétaire"@fr ;
        sh:description "Organisme ou personne qui détient des droits sur les données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 43 ;
    ] ;
    sh:property [
        sh:path geodcat:custodian ;
        sh:name "Gestionnaire"@fr ;
        sh:description "Organisme ou personne qui assume la maintenance des données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 44 ;
    ] ;
    sh:property [
        sh:path geodcat:distributor ;
        sh:name "Distributeur"@fr ;
        sh:description "Organisme ou personne qui assure la distribution des données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 45 ;
    ] ;
    sh:property [
        sh:path geodcat:originator ;
        sh:name "Commanditaire"@fr ;
        sh:description "Organisme ou personne qui est à l'origine de la création des données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 46 ;
    ] ;
    sh:property [
        sh:path geodcat:principalInvestigator ;
        sh:name "Maître d'œuvre"@fr ;
        sh:description "Organisme ou personne chargée du recueil des informations."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 47 ;
    ] ;
    sh:property [
        sh:path geodcat:processor ;
        sh:name "Intégrateur"@fr ;
        sh:description "Organisation ou personne qui a retraité les données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 48 ;
    ] ;
    sh:property [
        sh:path geodcat:resourceProvider ;
        sh:name "Fournisseur de la ressource"@fr ;
        sh:description "Organisme ou personne qui diffuse les données, soit directement soit par l'intermédiaire d'un distributeur."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 49 ;
    ]  ;
    sh:property [
        sh:path geodcat:user ;
        sh:name "Utilisateur"@fr ;
        sh:description "Organisme ou personne qui utilise les données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class foaf:Agent ;
        sh:order 50 ;
    ] ;
    sh:property [
        sh:path dcat:distribution ;
        sh:name "Distribution"@fr ;
        sh:description "Distribution."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dcat:Distribution ;
        sh:order 59 ;
    ] ;
    sh:property [
        sh:path dcat:landingPage ;
        sh:name "Page internet"@fr ;
        sh:description "URL de la fiche de métadonnées sur internet."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class foaf:Document ;
        sh:order 60 ;
    ] ;
    sh:property [
        sh:path foaf:page ;
        sh:name "Documentation"@fr ;
        sh:description "URL d'accès à une documentation rédigée décrivant la donnée."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class foaf:Document ;
        sh:order 61 ;
    ] ;
    sh:property [
        sh:path dct:isReferencedBy ;
        sh:name "Cité par"@fr ;
        sh:description "URL d'une publication qui utilise ou évoque les données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class rdfs:Resource ;
        sh:order 62 ;
    ] ;
    sh:property [
        sh:path dct:relation ;
        sh:name "Ressource liée"@fr ;
        sh:description "URL d'accès à une ressource en rapport avec les données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class rdfs:Resource ;
        sh:order 63 ;
    ] ;
    sh:property [
        sh:path dct:language ;
        sh:name "Langue des données"@fr ;
        sh:description "Langue·s des données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class dct:LinguisticSystem ;
        sh:order 80 ;
        plume:ontology <http://publications.europa.eu/resource/authority/language> ;
    ] ;
    sh:property [
        sh:path plume:relevanceScore ;
        sh:name "Score"@fr ;
        sh:description "Niveau de pertinence de la donnée. Plus le score est élevé plus la donnée sera mise en avant dans les résultats de recherche."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:integer ;
        sh:maxCount 1 ;
        sh:order 81 ;
    ] ;
    sh:property [
        sh:path dct:type ;
        sh:name "Type de jeu de données"@fr ;
        sh:description "Type de jeu de données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:order 82 ;
        plume:ontology <http://publications.europa.eu/resource/authority/dataset-type> ;
    ] ;
    sh:property [
        sh:path dct:identifier ;
        sh:name "Identifiant interne"@fr ;
        sh:description "Identifiant du jeu de données, attribué automatiquement par Plume."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:order 83 ;
    ] ;
    sh:property [
        sh:path plume:linkedRecord ;
        sh:name "Fiche distante"@fr ;
        sh:description "Configuration d'import des métadonnées depuis une fiche de catalogue INSPIRE."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class plume:LinkedRecord ;
        sh:maxCount 1 ;
        sh:order 84 ;
    ] ;
    sh:property [
        sh:path foaf:isPrimaryTopicOf ;
        sh:name "Informations sur les métadonnées"@fr ;
        sh:description "Métadonnées des métadonnées."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dcat:CatalogRecord ;
        sh:maxCount 1 ;
        sh:order 85 ;
    ].

plume:DistributionShape
    a sh:NodeShape ;
    sh:targetClass dcat:Distribution ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [
        sh:path dcat:accessURL ;
        sh:name "URL d'accès"@fr ;
        sh:description "URL de la page où est publiée cette distribution des données."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class rdfs:Resource ;
        sh:minCount 1 ;
        sh:order 1 ;
    ] ;
    sh:property [
        sh:path dct:title ;
        sh:name "Libellé"@fr ;
        sh:description "Nom de la distribution."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        sh:order 2 ;
    ] ;
    sh:property [
        sh:path dct:description ;
        sh:name "Description"@fr ;
        sh:description "Description de la distribution."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:uniqueLang true ;
        sh:order 3 ;
        plume:longText true ;
    ] ;
    sh:property [
        sh:path dcat:downloadURL ;
        sh:name "Lien de téléchargement direct"@fr ;
        sh:description "URL de téléchargement direct du ou des fichiers de la distribution."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class rdfs:Resource ;
        sh:order 4 ;
    ] ;
    sh:property [
        sh:path dct:issued ;
        sh:name "Date de publication"@fr ;
        sh:description "Date à laquelle la distribution a été diffusée."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 5 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        plume:inputMask "9999-99-99" ;
    ] ;
    sh:property [
        sh:path dct:modified ;
        sh:name "Date de dernière modification"@fr ;
        sh:description "Date de la dernière modification de la distribution."@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:date ;
        sh:maxCount 1 ;
        sh:order 12 ;
        sh:pattern "^[0-9]{4}[-][0-9]{2}[-][0-9]{2}$" ;
        plume:inputMask "9999-99-99" ;
    ] ;
    sh:property [
        sh:path dcatap:availability ;
        sh:name "Disponibilité"@fr ;
        sh:description "Niveau de disponibilité prévu pour la distribution, permettant d'apprécier le temps pendant lequel elle est susceptible de rester accessible."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:order 7 ;
        plume:ontology <http://publications.europa.eu/resource/authority/planned-availability> ;
    ] ;
    sh:property [
        sh:path adms:status ;
        sh:name "Statut"@fr ;
        sh:description "Maturité de la distribution."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:order 8 ;
        plume:ontology <http://publications.europa.eu/resource/authority/dataset-status>,
            <http://registre.data.developpement-durable.gouv.fr/plume/ISO19139ProgressCode> ;
    ] ;
    sh:property [
        sh:path dct:type ;
        sh:name "Type de distribution"@fr ;
        sh:description "Type de distribution."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:order 10 ;
        plume:ontology <http://publications.europa.eu/resource/authority/distribution-type> ;
    ] ;
    sh:property [
        sh:path dct:format ;
        sh:name "Format de fichier"@fr ;
        sh:description "Format de fichier ou extension."@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:MediaTypeOrExtent ;
        sh:maxCount 1 ;
        sh:order 11 ;
        plume:ontology <http://publications.europa.eu/resource/authority/file-type>,
            <http://registre.data.developpement-durable.gouv.fr/plume/IanaMediaType> ;
    ] ;
    sh:property [
        sh:path dcat:compressFormat ;
        sh:name "Format de compression"@fr ;
        sh:description "Format du fichier contenant les données sous une forme compressée, afin de réduire leur volume."@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:MediaType ;
        sh:maxCount 1 ;
        sh:order 12 ;
        plume:ontology <http://publications.europa.eu/resource/authority/file-type>,
            <http://registre.data.developpement-durable.gouv.fr/plume/IanaMediaType> ;
    ] ;
    sh:property [
        sh:path dcat:packageFormat ;
        sh:name "Format d'empaquatage"@fr ;
        sh:description "Format du fichier rassemblant les différents fichiers contenant les données pour permettre leur téléchargement conjoint."@fr ;
        sh:nodeKind sh:BlankNodeOrIRI ;
        sh:class dct:MediaType ;
        sh:maxCount 1 ;
        sh:order 13 ;
        plume:ontology <http://publications.europa.eu/resource/authority/file-type>,
            <http://registre.data.developpement-durable.gouv.fr/plume/IanaMediaType> ;
    ] ;
    sh:property [
        sh:path dcat:accessService ;
        sh:name "Service"@fr ;
        sh:description "Service donnant accès aux données."@fr ;
        sh:nodeKind sh:BlankNode ;
        sh:class dcat:DataService ;
        sh:order 14 ;
    ] ;
    sh:property [
        sh:path adms:representationTechnique ;
        sh:name "Mode de représentation géographique"@fr ;
        sh:description "Type de représentation technique de l'information géographique présentée par la distribution, le cas échéant."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:Concept ;
        sh:maxCount 1 ;
        sh:order 15 ;
        plume:ontology <http://inspire.ec.europa.eu/metadata-codelist/SpatialRepresentationType> ;
    ] ;
    sh:property [
        sh:path foaf:page ;
        sh:name "Documentation"@fr ;
        sh:description "Documentation ou page internet contenant des informations relative à la distribution."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class foaf:Document ;
        sh:order 16 ;
    ] ;
