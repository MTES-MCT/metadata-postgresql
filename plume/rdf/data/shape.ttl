snum:KindShape
    a sh:NodeShape ;
    sh:targetClass vcard:Kind ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [
		sh:path vcard:fn ;
		sh:name "Nom"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype xsd:string ;
        sh:maxCount 1 ;
        sh:order 1 ;
	] ;
    sh:property [
		sh:path vcard:hasEmail ;
