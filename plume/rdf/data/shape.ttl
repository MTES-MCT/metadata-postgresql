    sh:property [
		sh:path vcard:hasURL ;
		sh:name "Site internet"@fr ;
        sh:nodeKind sh:IRI ;
        sh:class owl:Thing ;
        sh:maxCount 1 ;
        sh:order 4 ;
	] ;
    sh:property [
