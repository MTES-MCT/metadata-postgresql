snum:LocationShape
    a sh:NodeShape ;
    sh:targetClass dct:Location ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [
		sh:path skos:inScheme ;
		sh:name "Index géographique"@fr ;
        sh:description "Type de lieu, index de référence pour l'identifiant (commune, département...)."@fr ;
        sh:nodeKind sh:IRI ;
        sh:class skos:ConceptScheme ;
        sh:maxCount 1 ;
        sh:order 0 ;
        snum:ontology snum:InseeGeoIndex ;
	] ;
    sh:property [
		sh:path dct:identifier ;
		sh:name "Code géographique"@fr ;
        sh:description "Code du département, code INSEE de la commune, etc."@fr ;
