        sh:datatype xsd:string ;
        sh:order 1 ;
	] ;
    sh:property [
		sh:path skos:prefLabel ;
		sh:name "Libellé"@fr ;
        sh:nodeKind sh:Literal ;
        sh:datatype rdf:langString ;
        sh:order 2 ;
        sh:uniqueLang true ;
	] ;
    sh:property [
		sh:path dcat:bbox ;
		sh:name "Rectangle d'emprise"@fr ;
