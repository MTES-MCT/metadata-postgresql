@prefix dct: <http://purl.org/dc/terms/> .
@prefix plume: <http://registre.data.developpement-durable.gouv.fr/plume/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<http://www.opengis.net/def/crs/EPSG/0> a skos:Concept,
        skos:ConceptScheme ;
    dct:identifier "OGC-EPSG" ;
    skos:inScheme plume:StandardsRegister ;
    skos:prefLabel "EPSG Coordinate reference system (OGC)"@en,
        "Système de référence de coordonnées EPSG (OGC)"@fr .

plume:StandardsRegister a skos:ConceptScheme ;
    skos:prefLabel "Standards register"@en,
        "Registre de standards"@fr .

